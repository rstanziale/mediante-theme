{
    "name": "mediante-theme",
    "version": "1.0.0",
    "description": "The 2020 theme for MEDIANTE",
    "author": "MEDIANTE",
    "license": "MIT License",
    "homepage": "https://linktr.ee/mediante",
    "scripts": {
        "update-mediante": "npm-run-all update-npm --parallel copy-css copy-scss copy-js",
        "update-npm": "npm update",
        "copy-css": "Xcopy /E /I node_modules\\bootstrap\\dist\\css\\bootstrap.min.css css\\lib\\",
        "copy-scss": "Xcopy /E /I node_modules\\bootstrap\\scss css\\scss\\bootstrap\\",
        "copy-js": "Xcopy /E /I node_modules\\bootstrap\\dist\\js\\bootstrap.bundle.min.js js\\lib\\"
    },
    "dependencies": {
        "@popperjs/core": "^2.10.2",
        "bootstrap": "^5.1.3",
        "npm-run-all": "^4.1.5"
    }
}
